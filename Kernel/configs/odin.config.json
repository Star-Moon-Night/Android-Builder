[
  {
    "kernelSource": {
      "name": "MIX4",
      "repo": "https://github.com/Diva-Room/Miku_kernel_xiaomi_modina",
      "branch": "Udon",
      "device": "odin",
      "defconfig": "vendor/lahaina-qgki-defconfig"
    },
      "toolchains": [
    {
      "name": "clang",
      "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/android12-release/clang-r416183b.tar.gz",
      "binaryEnv": ["./bin"]
    },
    {
    "name": "gcc",
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/heads/android12-release.tar.gz",
    "binaryEnv": ["bin"]
    }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
