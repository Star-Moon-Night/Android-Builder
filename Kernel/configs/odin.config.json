[
  {
    "kernelSource": {
      "name": "MIX4",
      "repo": "https://github.com/Diva-Room/Miku_kernel_xiaomi_modina",
      "branch": "Udon",
      "device": "odin",
      "defconfig": "vendor/lahaina-qgki_defconfig"
    },
      "toolchains": [
    {
      "name": "clang",
      "url": "https://storage.xzr.moe/horizon-clang/Horizon-Clang-17.0.6.tar.xz",
      "binaryEnv": ["/bin"]
    },
    {
    "name": "gcc",
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/heads/android12-release.tar.gz",
    "binaryEnv": ["bin"]
    }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
